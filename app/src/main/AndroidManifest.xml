<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.replate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.Signup.VolunteerSignup"></activity>
        <activity android:name=".activities.Signup.BusinessSignup" />
        <activity android:name=".activities.Dashboard.BusinessDashboard" />
        <activity android:name=".activities.Login.LoginBusinessActivity" />
        <activity android:name=".activities.Login.LoginVolunteerActivity" />
        <activity android:name=".activities.Login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.Dashboard.VolunteerDashBoard" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>